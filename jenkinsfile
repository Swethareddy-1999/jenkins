pipline {
     agent any
     stages {
	     stage("code") {
	         steps {
		         sh ''' 
                    echo "Hello, this is build"
                 '''
	         }	
	     }
	     stage('Test') {
            steps {
                script{
                 sh """
                    echo "Hello, this is test"
                 """
                }
            }
        }
        stage('deploy') {
            steps {
                script{
                 sh """
                    echo "Hello, this is test"
                 """
                }
            }
        } 
	    				
	 
      }
}